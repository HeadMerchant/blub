vec2 :: struct {
	x: float
	y: float

	# dot :: fn (self: ^vec2, other: vec2): float {
	# 	return self.x*other.x + self.y*other.y
	# }
}

printFloat :: fn(x: f32) {
	printf(c"%f\n", x)
}

printVec2 :: fn(v: vec2) {
	printf(c"vec2(")
	printFloat(v.x)
	printFloat(v.y)
	printf(c")")
}

main :: fn () {
	v := vec2(5., 4.)
	printVec2(v)
} 
